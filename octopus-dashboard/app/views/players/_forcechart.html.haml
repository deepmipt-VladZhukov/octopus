.panel.panel-default
  .panel-heading
    .panel-title= data['legend']['title']
  .panel-body
    %canvas{id: id, width: '500', height: '400'}

:javascript
  $(window).ready(() =>
    drawRadarChart(
      '#{id}', #{data.to_json}, ['#{@first.name} (#{@first.country})', '#{@second.name} (#{@second.country})']))